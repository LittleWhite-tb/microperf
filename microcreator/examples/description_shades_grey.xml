<?xml version="1.0"?>
<description>
  <kernel>
    <!--instruction part -->
    <!--allow instructions to be randomized in benchmarks-->
    <instruction>
      <operation>movss</operation>
      <memory>
        <register>
          <name>r1</name>
        </register>
        <offset>0</offset>
      </memory>
      <register>
        <phyName>%xmm</phyName>
        <min>0</min>
        <max>6</max>
      </register>
    </instruction>
    <instruction>
      <operation>addss</operation>
      <register>
        <phyName>%xmm</phyName>
        <min>7</min>
        <max>7</max>
      </register>
      <register>
        <phyName>%xmm</phyName>
        <min>7</min>
        <max>7</max>
      </register>
      <repetition>
        <min>1</min>
        <max>16</max>
      </repetition>
    </instruction>
    <!--end instruction part -->
    <induction>
      <register>
        <name>r1</name>
      </register>
      <offset>4</offset>
      <stride>
        <min>1</min>
        <max>4</max>
      </stride>
    </induction>
    <induction>
      <register>
        <phyName>%eax</phyName>
      </register>
      <offset>1</offset>
      <not_affected_unroll/>
    </induction>
    <induction>
      <register>
        <name>r0</name>
      </register>
      <offset>-1</offset>
      <linked>
        <register>
          <name>r1</name>
        </register>
      </linked>
      <last_induction/>
    </induction>
    <branch_information>
      <label>L6</label>
      <test>jge</test>
    </branch_information>
    <unrolling>
      <min>1</min>
      <max>8</max>
      <progress>1</progress>
    </unrolling>
  </kernel>
  <hardware_detector>
    <execute>../microdetect/microdetect ../microdetect/data/args.c ../microdetect/output</execute>
    <information_file>../microdetect/output</information_file>
  </hardware_detector>
</description>
