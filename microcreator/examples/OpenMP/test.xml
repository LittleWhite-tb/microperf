<?xml version="1.0"?>
<description>
  <kernel>
    <insert_code>
      <prologue>examples/OpenMP/prologue.c</prologue>
    </insert_code>
  </kernel>
  <kernel>
    <loop_info>
      <count_iteration>
        <induction>i</induction>
        <increment>1</increment>
        <min>0</min>
        <max>20</max>
        <register>
          <name>r0</name>
          <offset>1</offset>
        </register>
      </count_iteration>
      <count_iteration>
        <induction>j</induction>
        <increment>4</increment>
        <minVariable>m</minVariable>
        <max>30</max>
        <register>
          <name>r1</name>
          <offset>4</offset>
        </register>
      </count_iteration>
      <omp_option>
        <firstprivate>
          <name>x</name>
          <min>0</min>
          <max>7</max>
        </firstprivate>
        <firstprivate>
          <name>r0</name>
        </firstprivate>
        <shared>
          <name>x9</name>
        </shared>
      </omp_option>
      <schedule>
        <type>static</type>
        <size>2</size>
      </schedule>
    </loop_info>
    <instruction>
      <operation>movss</operation>
      <memory>
        <register>
          <name>r0</name>
        </register>
        <offset>0</offset>
      </memory>
      <register>
        <phyName>%xmm</phyName>
        <min>0</min>
        <max>8</max>
      </register>
    </instruction>
    <instruction>
      <operation>movss</operation>
      <memory>
        <register>
          <name>r1</name>
        </register>
        <offset>0</offset>
      </memory>
      <register>
        <phyName>%xmm</phyName>
        <min>0</min>
        <max>8</max>
      </register>
    </instruction>
    <unrolling>
      <min>1</min>
      <max>8</max>
      <progress>1</progress>
    </unrolling>
  </kernel>
  <kernel>
    <insert_code>
      <epilogue>examples/OpenMP/epilogue.c</epilogue>
    </insert_code>
  </kernel>
  <hardware_detector>
    <execute>../microdetect/microdetect ../microdetect/data/args.c ../microdetect/output</execute>
    <information_file>../microdetect/output</information_file>
  </hardware_detector>
</description>
