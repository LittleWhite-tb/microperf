<?xml version="1.0"?>
<description>
  <kernel>
    <insert_code>
      <prologue>examples/OpenMP/prologue.c</prologue>
    </insert_code>
  </kernel>
  <kernel>
    <loop_info>
      <count_iteration>
        <induction>i</induction>
        <minVariable>n</minVariable>
        <maxVariable>m</maxVariable>
      </count_iteration>
      <induction>
        <register>
          <name>r0</name>
        </register>
        <increment>4</increment>
        <offset>8</offset>
        <no_generate/>
      </induction>
      <omp_option>
        <shared>
          <name>r</name>
          <min>0</min>
          <max>7</max>
        </shared>
        <firstprivate>
          <name>r</name>
          <min>0</min>
          <max>7</max>
        </firstprivate>
        <firstprivate>
          <name>x0</name>
        </firstprivate>
        <lastprivate>
          <name>x1</name>
        </lastprivate>
        <lastprivate>
          <name>x2</name>
        </lastprivate>
        <private>
          <name>x0</name>
        </private>
      </omp_option>
    </loop_info>
    <instruction>
      <operation>movss</operation>
      <memory>
        <register>
          <name>r0</name>
        </register>
        <offset>0</offset>
      </memory>
      <register>
        <phyName>%xmm</phyName>
        <min>0</min>
        <max>8</max>
      </register>
    </instruction>
    <unrolling>
      <min>1</min>
      <max>8</max>
      <progress>1</progress>
    </unrolling>
  </kernel>
  <kernel>
    <insert_code>
      <epilogue>examples/OpenMP/epilogue.c</epilogue>
    </insert_code>
  </kernel>
  <hardware_detector>
    <execute>../microdetect/microdetect ../microdetect/data/args.c ../microdetect/output</execute>
    <information_file>../microdetect/output</information_file>
  </hardware_detector>
</description>
